import scrapy


class TutorialItem(scrapy.Item):
    # define the fields for your item here like:
    # name = scrapy.Field()
    pass
class QueryItem(scrapy.Item):
    data = scrapy.Field()
class TencentItem(scrapy.Item):
    platform = scrapy.Field()
    tid = scrapy.Field()
    title = scrapy.Field()
    url = scrapy.Field()
    category = scrapy.Field()
    newspaper = scrapy.Field()
    author = scrapy.Field()
    time = scrapy.Field()
    desc = scrapy.Field()
    topic = scrapy.Field()
    keywords = scrapy.Field()
